import("//build/config/android/rules.gni")

android_resources("ambient_sdk_resources") {
  v14_skip = true
  resource_dirs = [ "//third_party/ambientsdk/ambient/java/res" ]
  custom_package = "com.cyanogen.ambientsdk"
}

android_java_prebuilt("ambient_sdk_java") {
  deps = [
    ":ambient_sdk_resources",
  ]
  jar_path = "//third_party/ambientsdk/ambient/java/lib/ambientsdk-1.5.11.jar"
  proguard_preprocess = true
  proguard_config = "//third_party/ambientsdk/proguard.flags"
}
